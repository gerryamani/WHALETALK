#include <iostream>
#include <vector>
#include <string>

int main (){
  
  int i,j,k; 
  std::string input = "turpentine and turtles\n";
  std::vector<char> vowels = {'a','e','i','o','u'}; 
  std::vector<char> result ;
  
  
  for ( i=0 ; i < input.size() ; i++ ){
  
  for ( j=0; j < vowels.size() ; j++){
    
    if (input[i] == vowels[j]){
      
    result.push_back(input[i]);
    
      
    }
    
  }
  
}
  for ( k=0; k< result.size(); k++ ){
    std::cout << result[k];
  }
 
}

